# Elitesgen Academy Mobile Apps Environment Variables

# API Configuration
EXPO_PUBLIC_API_URL=http://localhost:8000
EXPO_PUBLIC_ENV=development

# Authentication
EXPO_PUBLIC_JWT_SECRET_KEY=your-jwt-secret-key-here

# Third-party Services
EXPO_PUBLIC_SENTRY_DSN=your-sentry-dsn-here
EXPO_PUBLIC_ANALYTICS_KEY=your-analytics-key-here

# Feature Flags
EXPO_PUBLIC_ENABLE_ANALYTICS=false
EXPO_PUBLIC_ENABLE_CRASH_REPORTING=false
EXPO_PUBLIC_ENABLE_DEBUG_MENU=true

# Backend Services
EXPO_PUBLIC_BACKEND_VERSION=v1
EXPO_PUBLIC_WS_URL=ws://localhost:8001

# Development
EXPO_PUBLIC_DEBUG_MODE=true
EXPO_PUBLIC_LOG_LEVEL=debug