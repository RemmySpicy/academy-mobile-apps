{
  "name": "academy-instructors-app",
  "version": "2.0.0",
  "description": "Academy Management System - Instructors & Coordinators Mobile App",
  "main": "index.js",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android --dev-client",
    "ios": "expo run:ios --dev-client",
    "web": "expo start --web",
    "build:development": "eas build --profile development",
    "build:preview": "eas build --profile preview",
    "build:production": "eas build --profile production",
    "dev-client:android": "expo install --dev-client && expo run:android",
    "dev-client:ios": "expo install --dev-client && expo run:ios",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf node_modules .expo dist build",
    "prebuild": "expo prebuild --clean",
    "postinstall": "expo install --fix"
  },
  "dependencies": {
    "@expo/vector-icons": "14.1.0",
    "@react-navigation/bottom-tabs": "6.6.1",
    "@react-navigation/native": "6.1.18",
    "@react-navigation/native-stack": "6.11.0",
    "@react-navigation/drawer": "6.7.2",
    "expo": "53.0.0",
    "expo-build-properties": "0.14.8",
    "expo-constants": "17.1.7",
    "expo-dev-client": "5.2.4",
    "expo-device": "7.1.4",
    "expo-font": "13.3.2",
    "expo-linear-gradient": "14.1.5",
    "expo-location": "18.1.6",
    "expo-notifications": "0.31.4",
    "expo-secure-store": "14.2.3",
    "expo-splash-screen": "0.30.10",
    "expo-status-bar": "2.2.3",
    "expo-system-ui": "5.0.10",
    "react": "19.1.0",
    "react-native": "0.80.2",
    "react-native-gesture-handler": "2.24.0",
    "react-native-reanimated": "3.17.4",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "4.11.1",
    "react-native-svg": "15.11.2",
    "nativewind": "2.0.11",
    "clsx": "2.1.1",
    "zustand": "5.0.2",
    "react-hook-form": "7.62.0",
    "@hookform/resolvers": "3.9.0",
    "yup": "1.4.0",
    "date-fns": "4.1.0",
    "@react-native-async-storage/async-storage": "2.1.2"
  },
  "devDependencies": {
    "@babel/core": "7.24.0",
    "@types/react": "19.1.9",
    "@typescript-eslint/eslint-plugin": "8.0.0",
    "@typescript-eslint/parser": "8.0.0",
    "eslint": "8.0.0",
    "eslint-config-expo": "9.2.0",
    "jest": "29.0.0",
    "typescript": "5.6.2",
    "tailwindcss": "3.3.2"
  },
  "keywords": [
    "academy",
    "instructors",
    "coordinators",
    "mobile",
    "react-native",
    "expo",
    "education"
  ],
  "author": "Academy Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RemmySpicy/academy-mobile-apps.git",
    "directory": "academy-instructors-app"
  },
  "bugs": {
    "url": "https://github.com/RemmySpicy/academy-mobile-apps/issues"
  },
  "homepage": "https://github.com/RemmySpicy/academy-mobile-apps/tree/main/academy-instructors-app#readme",
  "expo": {
    "install": {
      "exclude": [
        "react-native-safe-area-context"
      ]
    }
  },
  "private": true
}
