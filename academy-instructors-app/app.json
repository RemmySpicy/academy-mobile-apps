{
  "expo": {
    "name": "Egen Academy Team",
    "slug": "academy-instructors-app",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.elitesgen.academy.instructors",
      "jsEngine": "jsc"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.elitesgen.academy.instructors",
      "jsEngine": "jsc"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-dev-client",
      [
        "expo-build-properties",
        {
          "android": {
            "newArchEnabled": false,
            "enableHermes": false
          },
          "ios": {
            "newArchEnabled": false,
            "enableHermes": false
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": false
    },
    "extra": {
      "eas": {
        "projectId": "academy-instructors-app"
      }
    },
    "owner": "elitesgen-academy",
    "scheme": "academy-instructors"
  }
}