{
  "name": "academy-instructors-app",
  "version": "2.0.0",
  "description": "Elitesgen Academy Team - Instructors & Coordinators Mobile App",
  "main": "index.js",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android --dev-client",
    "ios": "expo run:ios --dev-client",
    "web": "expo start --web",
    "build:development": "eas build --profile development",
    "build:preview": "eas build --profile preview",
    "build:production": "eas build --profile production",
    "dev-client:android": "expo install --dev-client && expo run:android",
    "dev-client:ios": "expo install --dev-client && expo run:ios",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "test:coverage": "jest --coverage --passWithNoTests",
    "clean": "rm -rf node_modules .expo dist build",
    "prebuild": "expo prebuild --clean",
    "postinstall": "expo install --fix"
  },
  "dependencies": {
    "@academy/mobile-shared": "file:../shared"
  },
  "devDependencies": {
    "@babel/core": "7.28.0",
    "babel-preset-expo": "~13.0.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint-config-expo": "9.2.0",
    "tailwindcss": "3.3.2"
  },
  "keywords": ["elitesgen", "academy", "instructors", "coordinators", "mobile", "react-native", "expo", "education"],
  "author": "Elitesgen Academy Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RemmySpicy/academy-mobile-apps.git",
    "directory": "academy-instructors-app"
  },
  "bugs": {
    "url": "https://github.com/RemmySpicy/academy-mobile-apps/issues"
  },
  "homepage": "https://github.com/RemmySpicy/academy-mobile-apps/tree/main/academy-instructors-app#readme",
  "expo": {
    "install": {
      "exclude": ["react-native-safe-area-context"]
    }
  },
  "private": true
}
