{
  "compilerOptions": {
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@academy/mobile-shared": [
        "../shared/src"
      ],
      "@academy/mobile-shared/*": [
        "../shared/src/*"
      ]
    }
  },
  "extends": "expo/tsconfig.base",
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "nativewind-env.d.ts"
  ],
  "exclude": [
    "existing-code/**/*"
  ]
}